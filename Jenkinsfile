pipeline {
    agent { docker { image 'php' } }
    stages {
        stage('prep') {
            steps {
                echo "starting prep phase"
                echo "finish prep phase"
            }
        }
        stage('build') {
            steps {
                sh 'php --version'
            }
        }
        stage('test') {
            steps {
                sh 'php --version'
            }
        stage('deploy') {
            steps {
                sh 'php --version'
            }
            
        }
    }
}
